{
  "name": "kuwomusicapi",
  "version": "1.0.0",
  "description": "Egg for 酷我音乐API;酷我音乐API Node.js版",
  "private": true,
  "keywords": [
    "酷我音乐API Node.JS版",
    "音乐API",
    "酷我音乐API"
  ],
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --port 7002 --title=egg-server-server-egg",
    "stop": "egg-scripts stop --title=egg-server-server-egg",
    "docs": "docsify serve ./docs",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "docsify": "^4.12.2",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.6.0",
    "iconv-lite": "^0.6.3",
    "needle": "^3.2.0",
    "process": "^0.11.10",
    "uuid": "^8.3.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/needle": "^3.2.0",
    "@types/node": "^7.10.14",
    "@types/supertest": "^2.0.0",
    "@types/tunnel": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "docsify-cli": "^4.4.4",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^7.28.0",
    "eslint-config-egg": "^8.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QiuYaohong/kuwoMusicApi.git"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "qyhqiu",
  "license": "ISC"
}
